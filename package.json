{
  "name": "sniperfox",
  "version": "1.0.0",
  "description": "",
  "author": "aJV99",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "test -f .env || test -n \"$RENDER\" || (echo 'Error: .env not found. Copy .env.example to .env and fill in your values.' && exit 1)",
    "build": "tsc",
    "start": "node ./dist/src/bot.js",
    "dev": "nodemon --exec ts-node ./src/bot.ts",
    "deploy": "ts-node ./src/deploy-commands.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^1.13.1",
    "@discordjs/rest": "^2.6.0",
    "discord-api-types": "^0.38.38",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
